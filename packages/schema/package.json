{
  "name": "@mediaurl/schema",
  "description": "MediaURL JSON Schema",
  "version": "1.1.3",
  "main": "dist",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rm -rf dist && mkdir -p dist && npm run build:tsc && npm run build:schema && npm run build:types",
    "build:tsc": "tsc",
    "build:schema": "node ./buildSchema.js",
    "build:types": "node ./buildTypes.js",
    "test": "jest"
  },
  "dependencies": {
    "ajv": "7.0.3"
  },
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@types/node": "14.14.22",
    "jest": "26.6.3",
    "js-yaml": "4.0.0",
    "json-schema-ref-parser": "9.0.7",
    "json-schema-to-typescript": "10.1.3",
    "ts-jest": "26.5.0",
    "typescript": "4.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
