{
  "name": "@mediaurl/schema",
  "description": "MediaURL JSON Schema",
  "version": "1.0.0-alpha.2",
  "main": "dist",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rm -rf dist && mkdir -p dist && npm run build:tsc && npm run build:schema && npm run build:types",
    "build:tsc": "tsc",
    "build:schema": "node ./buildSchema.js",
    "build:types": "node ./buildTypes.js",
    "test": "jest"
  },
  "dependencies": {
    "ajv": "^7.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^14.14.16",
    "jest": "^26.4.2",
    "js-yaml": "^3.14.1",
    "json-schema-ref-parser": "^9.0.6",
    "json-schema-to-typescript": "^10.0.3",
    "ts-jest": "^26.3.0",
    "typescript": "^4.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
